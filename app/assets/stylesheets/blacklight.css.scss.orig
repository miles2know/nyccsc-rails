$blue-light:    lighten($blue, 10%);
$blue-dark:     darken($blue, 5%);
$blue-darker:   darken($blue, 10%);


// Secondary Colors
$brand-secondary:       $blue;
$btn-secondary-color:   #fff;
$btn-secondary-bg:      $brand-secondary;
$btn-secondary-border:  darken($btn-secondary-bg, 5%);


// More Bootstrap 
$body-bg:         #f5f5f5;
$headings-color:  #007d99;
$brand-primary:   $green;

$navbar-border-radius: 0px;

$navbar-margin-bottom: 0px;
$navbar-default-bg: $blue;
$navbar-default-border: $blue;
$navbar-default-link-color: #e2e2e2;

$navbar-default-link-hover-bg: lighten($blue, 5%);
$navbar-default-link-hover-color: #fff;

$navbar-default-link-active-bg: lighten($blue, 5%);
$navbar-default-link-active-color: #fff;

$panel-default-heading-bg: #fff;
$well-bg: #fff;

// Mixins
@mixin bg-color ($bg-color,$font-color:false) {
  background-color: $bg-color;
  @if ($font-color) {
    color:$font-color;
  }
}

@import 'bootstrap';
@import 'blacklight/blacklight';

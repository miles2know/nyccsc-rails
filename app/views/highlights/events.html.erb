<section class="page-title">
  <div class="container">
    <h1>Events</h1>
  </div>
</section>

<section>
  <div class="container">

    <div class="row">
      <div class="col-sm-12">

        <%   @data.each_solution do |item| %>
          <h3><%= item['label'].to_s.html_safe %></h3>

          <% if item.bound?('resourceURI') %>
            <p><%= item['resourceURI'].to_s.html_safe %></p>
          <% end %>

          <% if item.bound?('location') %>
            <p><%= item['location'].to_s.html_safe %></p>
          <% end %>

          <% if item.bound?('start') %>
            <p><%= item['start'].to_s.html_safe %></p>
          <% end %>

          <% if item.bound?('end') %>
            <p><%= item['end'].to_s.html_safe %></p>
          <% end %>

          <% if item.bound?('description') %>
            <p><%= item['description'].to_s.html_safe %></p>
          <% end %>
        
        <% end %>

      </div>
    </div>

  </div>
</section>